
<header>
  <div class="navbar navbar-fixed-top" role="navigation">
    <div class="container-fluid">
      <div class="navbar-header">
          
        <%= link_to root_path do %>
         <%= image_tag "logo.png", class: "logo" %>
        <% end %>

        <%= link_to "Regístrate", new_usuario_path, class: 'btn btn-default' %>
        <%= link_to "Usuarios", usuarios_path, class: 'btn btn-default' %>
<%= link_to "Categorias", categoria_path, class: 'btn btn-default' %>

		<% if session[:usuario_id]==nil %>
