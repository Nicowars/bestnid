<h1>Estadisticas</h1>

</br>
	<% lista=params[:lista] %>
	<table BORDER=2 WIDTH= 600 BORDERCOLOR="red">
		<tr>
			<td WIDTH=200 ALIGN=CENTER BGCOLOR="red" style="color:white"> Nombre </td>
			<td WIDTH=200 ALIGN=CENTER BGCOLOR="red" style="color:white"> Mail </td>
			<td WIDTH=200 ALIGN=CENTER BGCOLOR="red" style="color:white"> Fecha de registro </td>
		</tr>
		<% lista.each do |l| %>
			<tr>
			<td WIDTH=200 ALIGN=CENTER > <%= l.nombre %> </td>
				<td WIDTH=200 ALIGN=CENTER > <%= l.mail %> </td>
				<td WIDTH=200 ALIGN=CENTER > <%= l.created_at %> </td>
			</tr>
		<% end %>
	</table>
	
</br>
<%= link_to "Volver", estadisticas_show_path , class: 'btn btn-primary' %>